{
  // The following will hide the js and map files in the editor
  "files.exclude": {
    "**/*.js": true,
    "**/*.map": true,
    "dist": true,
  },
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "interactor",
    "Merkle"
  ]
}
